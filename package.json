{
  "name": "@anoesj/eslint-config-vue-ts",
  "version": "0.4.0",
  "description": "Anoesj's ESLint config for Vue and Nuxt projects with TypeScript",
  "author": "Anoesj Sadraee (https://github.com/anoesj)",
  "license": "MIT",
  "homepage": "https://github.com/anoesj/eslint-config-vue-ts#readme",
  "packageManager": "pnpm@10.17.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/anoesj/eslint-config-vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/anoesj/eslint-config-vue-ts/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "build": "tsdown",
    "watch": "tsdown --watch",
    "inspect": "@eslint/config-inspector",
    "release": "bumpp && pnpm publish --access public"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@eslint/js": "^9.35.0",
    "@stylistic/eslint-plugin": "^5.3.1",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.4.0",
    "typescript-eslint": "^8.44.0",
    "vue-eslint-parser": "^10.2.0"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^1.2.0",
    "bumpp": "^10.2.3",
    "eslint": "^9.35.0",
    "jiti": "^2.5.1",
    "tsdown": "^0.15.2",
    "typescript": "^5.9.2"
  }
}

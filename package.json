{
  "name": "@anoesj/eslint-config-vue-ts",
  "version": "0.2.13",
  "description": "Anoesj's ESLint config for Vue and Nuxt projects with TypeScript",
  "author": "Anoesj Sadraee (https://github.com/anoesj)",
  "license": "MIT",
  "homepage": "https://github.com/anoesj/eslint-config-vue-ts#readme",
  "packageManager": "pnpm@9.15.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/anoesj/eslint-config-vue-ts.git"
  },
  "bugs": {
    "url": "https://github.com/anoesj/eslint-config-vue-ts/issues"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "scripts": {
    "build": "tsup --clean --dts",
    "watch": "tsup --watch",
    "inspect": "@eslint/config-inspector",
    "release": "bumpp && pnpm publish --access public"
  },
  "peerDependencies": {
    "eslint": ">= 9"
  },
  "dependencies": {
    "@eslint/js": "^9.17.0",
    "@stylistic/eslint-plugin": "^2.12.1",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.13.0",
    "typescript-eslint": "^8.18.0",
    "vue-eslint-parser": "^9.4.3"
  },
  "devDependencies": {
    "@eslint/config-inspector": "^0.7.0",
    "bumpp": "^9.9.1",
    "eslint": "^9.17.0",
    "jiti": "^2.4.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
